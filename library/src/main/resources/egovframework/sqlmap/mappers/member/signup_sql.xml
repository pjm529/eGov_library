<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="SignUp">

	<select id="idCheck" resultType="int">
		<![CDATA[
			SELECT
				count(*)
			FROM 
				member
			where
				user_id = #{userId}
		]]>
	</select>
	
	
</mapper>
